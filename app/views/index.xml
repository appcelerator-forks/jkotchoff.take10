<Alloy>
  <Collection src="deca" />
	<Window id="myDecas">
	  <View class="container">
      <Button onClick="createDeca" title="Create a Deca"/>
  
      <TableView id="myDecasList" dataTransform="transformData" dataCollection="deca">
        <TableViewRow onClick="showDeca" decaId="{decaId}" title="{name}" />
      </TableView>
    </View>
  </Window>
</Alloy>